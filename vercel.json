{
    "rewrites": [
        {
            "source": "/",
            "destination": "/mosaic-magazine-mockup.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        }
    ]
}